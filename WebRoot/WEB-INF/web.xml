<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name></display-name>	

  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  
  <session-config>
  	<session-timeout>10</session-timeout>
  </session-config>
  
  <servlet>
  	<servlet-name>cookies</servlet-name>
  	<servlet-class>cn.com.servlet.cookies.cookies</servlet-class>
  </servlet>
  <servlet>
  	<servlet-name>deletecookie</servlet-name>
  	<servlet-class>cn.com.servlet.cookies.deletecookie</servlet-class>
  </servlet>
  
  <servlet-mapping>
  	<servlet-name>cookies</servlet-name>
  	<url-pattern>/cookies</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>deletecookie</servlet-name>
  	<url-pattern>/deletecookie</url-pattern>
  </servlet-mapping>


</web-app>
